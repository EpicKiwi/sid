{
  "name": "sid",
  "description": "Simple Instant Deployment - CLI tool for quick and simple deployment",
  "version": "0.0.0",
  "author": "EpicKiwi @EpicKiwi",
  "bin": {
    "sid": "./bin/run"
  },
  "bugs": "https://github.com/EpicKiwi/sid/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "cli-ux": "^5.5.0",
    "fs-extra": "^9.0.1",
    "ssh2": "^0.8.9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/EpicKiwi/sid",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "sid",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "EpicKiwi/sid",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
